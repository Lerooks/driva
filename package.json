{
  "name": "driva",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "yarn start:mock & dotenv -e .env.development -- nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "start:mock": "node external-services.js",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test:unit": "dotenv -e .env.test -- jest --runInBand --config ./jest.unit.config.ts",
    "test:int": "dotenv -e .env.test -- jest --runInBand --config ./jest.int.config.ts",
    "test:coverage": "dotenv -e .env.test -- jest --config ./jest.coverage.config.ts --runInBand --coverage",
    "test:unit:watch": "dotenv -e .env.test -- jest --runInBand --config ./jest.unit.config.ts --watch",
    "test:int:watch": "dotenv -e .env.test -- jest --runInBand --config ./jest.int.config.ts --watch",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "prisma:generate:dev": "dotenv -e .env.development -- prisma generate --schema src/shared/infrastructure/persistence/database/prisma/schema.prisma",
    "prisma:migrate:dev": "dotenv -e .env.development -- prisma migrate dev --schema src/shared/infrastructure/persistence/database/prisma/schema.prisma",
    "prisma:generate:test": "dotenv -e .env.test -- prisma generate --schema src/shared/infrastructure/persistence/database/prisma/schema.prisma",
    "prisma:migrate:test": "dotenv -e .env.test -- prisma migrate deploy --schema src/shared/infrastructure/persistence/database/prisma/schema.prisma",
    "prisma:seed:dev": "dotenv -e .env.development -- ts-node src/shared/infrastructure/persistence/database/prisma/prisma.seeder.ts",
    "prisma:seed:test": "dotenv -e .env.test -- ts-node src/shared/infrastructure/persistence/database/prisma/prisma.seeder.ts"
  },
  "dependencies": {
    "@nestjs-modules/ioredis": "^2.0.2",
    "@nestjs/axios": "^4.0.0",
    "@nestjs/common": "^11.0.1",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.0.1",
    "@nestjs/mapped-types": "*",
    "@nestjs/microservices": "^11.1.0",
    "@nestjs/platform-express": "^11.0.1",
    "@nestjs/swagger": "^11.1.4",
    "@prisma/client": "6.6.0",
    "amqp-connection-manager": "^4.1.14",
    "amqplib": "^0.10.7",
    "axios": "^1.9.0",
    "bcryptjs": "^3.0.2",
    "express": "^5.1.0",
    "ioredis": "^5.6.1",
    "luxon": "^3.6.1",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "swagger-ui-express": "^5.0.1",
    "uuid": "^11.1.0",
    "zod": "^3.24.3"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.18.0",
    "@faker-js/faker": "^9.7.0",
    "@nestjs/cli": "^11.0.0",
    "@nestjs/schematics": "^11.0.0",
    "@nestjs/testing": "^11.0.1",
    "@swc/cli": "^0.6.0",
    "@swc/core": "^1.10.7",
    "@types/amqplib": "^0.10.7",
    "@types/bcryptjs": "^3.0.0",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.14",
    "@types/luxon": "^3.6.2",
    "@types/node": "^22.10.7",
    "@types/supertest": "^6.0.2",
    "@types/uuid": "^10.0.0",
    "dotenv-cli": "^8.0.0",
    "eslint": "^9.26.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.2",
    "eslint-plugin-unused-imports": "^4.1.4",
    "globals": "^16.0.0",
    "jest": "^29.7.0",
    "prettier": "^3.4.2",
    "prisma": "^6.6.0",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.20.0"
  }
}
